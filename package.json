  "lint-staged": {
    "{src}/**/*.{scss}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "{src,test}/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
